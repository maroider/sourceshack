{% extends "base" %}
{% block title %} {{ owner }}/{{ name }} {% endblock title %}
{% block head %}
  {{ super() }}
{% endblock head %}
{%block body%}
  <h1>{{ owner }}/{{ name }}</h1>
  <ul>
    {% for entry in tree %}
      <li>
        {% if entry.is_file %}
            📄
        {% elif entry.is_dir %}
            📁
        {% else %}
            ⁉
        {% endif %}
        {{ entry.name }}
      </li>
    {% endfor %}
  </ul>
{%endblock body%}
